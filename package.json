  "peerDependencies": {
    "axios": "^0.18.0",
    "moment": "^2.24.0",
    "popper.js": "^1.14.7",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
